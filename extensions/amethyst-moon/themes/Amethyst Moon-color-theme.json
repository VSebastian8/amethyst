	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#D4D4E4"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control",
			],
			"settings": {
				"foreground": "#7374D3"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator",
				"meta.tag",
				
			],
			"settings": {
				"foreground": "#82AAFF",
				"fontStyle": "bold",

			}
		},
		{ 	
			"name": "Other Keywords",
